{
    "builds": [{
        "src": "core/wsgi.py",
        "use": "@ardnt/vercel-python",
        "config": { "maxLambdaSize": "15mb" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "core/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static"
        }
    ]
}